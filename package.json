{
  "name": "neno",
  "version": "1.0.0",
  "description": "NENO is an app that helps you creating your personal knowledge graph.",
  "type": "module",
  "dependencies": {
    "@editorjs/editorjs": "2.24.3",
    "@editorjs/header": "^2.6.2",
    "@editorjs/list": "^1.7.0",
    "@tippyjs/react": "^4.2.6",
    "@types/cheerio": "^0.22.31",
    "@types/chrome": "^0.0.183",
    "@types/d3": "^7.4.0",
    "@types/express": "^4.17.13",
    "@types/react": "^18.0.8",
    "@types/wicg-file-system-access": "^2020.9.5",
    "archiver": "^5.3.1",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.11",
    "commander": "^9.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "d3": "^7.4.4",
    "eslint-plugin-react": "^7.29.3",
    "express": "^4.18.0",
    "express-history-api-fallback": "^2.2.1",
    "express-session": "^1.17.2",
    "idb-keyval": "^6.1.0",
    "node-2fa": "^2.0.3",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "tippy.js": "^6.3.7",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "concurrently": "^7.1.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "style-loader": "^3.3.1",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "dev-be": "npx webpack --config browser-extension/webpack.common.js --watch",
    "build-browser-extension": "npx webpack --config browser-extension/webpack.common.js",
    "compile-typescript": "npx tsc",
    "start-server-dev": "node dist/backend/index.js --http-port 8080 --session-secret 000",
    "start-server-dev-tls": "node dist/backend/index.js --https-port 8080 --use-https --cert-path ~/.neno/server.cert --cert-key-path ~/.neno/server.key",
    "build-frontend-production": "npx webpack --config frontend/webpack.production.js",
    "build-frontend-development": "npx webpack --config frontend/webpack.development.js",
    "build": "npm run compile-typescript && npm run build-frontend-production",
    "start-dev-server-frontend": "npx webpack serve --config frontend/webpack.development.js",
    "start": "npm run build && npm run start-server-dev",
    "start-tls": "npm run build && npm run start-server-dev-tls",
    "dev": "concurrently -i --default-input-target 0 --kill-others \"npm run compile-typescript && npm run start-server-dev\" \"npm run build-frontend-development\""
  },
  "author": "Sebastian Zimmer",
  "license": "Apache License 2.0"
}
