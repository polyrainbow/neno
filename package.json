{
  "name": "neno",
  "version": "1.0.0",
  "description": "NENO is an app that helps you creating your personal knowledge graph.",
  "type": "module",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@editorjs/attaches": "github:SebastianZimmer/attaches#ab6819fe43e7aedf274bd2d391921962f7557359",
    "@editorjs/code": "^2.6.0",
    "@editorjs/editorjs": "2.19.1",
    "@editorjs/header": "^2.6.0",
    "@editorjs/image": "github:SebastianZimmer/image#421a176830dc9eea8759e09f16866aa959176843",
    "@editorjs/link": "github:SebastianZimmer/link#016b6eb007d70ef3d47bd6bf6b3769b22c5a82e3",
    "@editorjs/list": "^1.6.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/express": "^4.17.9",
    "archiver": "^5.0.2",
    "babel-loader": "^8.1.0",
    "bcryptjs": "^2.4.3",
    "commander": "^7.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "d3": "^6.3.1",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "idb-keyval": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^1.0.4",
    "node-2fa": "^2.0.1",
    "qrcode-terminal": "^0.12.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "react-tippy": "^1.4.0",
    "readline": "^1.3.0",
    "readline-sync": "^1.4.10",
    "tippy.js": "^6.2.7",
    "ts-loader": "^8.0.13",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "url-metadata": "^2.5.0",
    "uuid": "^8.3.2",
    "webpack": "^5.11.1",
    "webpack-dev-server": "^3.11.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "style-loader": "^2.0.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "visualize-bundles": "npx webpack --config frontend/webpack.production.js --profile --json > stats.json && npx webpack-bundle-analyzer stats.json frontend/assets",
    "compile-typescript": "npx tsc",
    "start-server": "node dist/backend/index.js --port 8080",
    "start-server-tls": "node dist/backend/index.js --https-port 8080 --use-https --cert-path ~/.neno/server.cert --cert-key-path ~/.neno/server.key",
    "build-frontend-production": "npx webpack --config frontend/webpack.production.js",
    "build-frontend-development": "npx webpack --config frontend/webpack.development.js",
    "build": "npm run compile-typescript && npm run build-frontend-production",
    "start-dev-server-frontend": "npx webpack serve --config frontend/webpack.development.js",
    "start": "npm run build && npm run start-server",
    "start-tls": "npm run build && npm run start-server-tls",
    "dev": "concurrently --kill-others \"npm run compile-typescript && npm run start-server\" \"npm run build-frontend-development\""
  },
  "author": "Sebastian Zimmer",
  "license": "ISC"
}
