{
  "name": "neno",
  "version": "4.4.1",
  "description": "NENO is a note-taking app that helps you create your personal knowledge graph.",
  "type": "module",
  "dependencies": {
    "ajv": "^8.11.2",
    "archiver": "^5.3.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "node-2fa": "^2.0.3",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@tippyjs/react": "^4.2.6",
    "@types/chrome": "^0.0.203",
    "@types/d3": "^7.4.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.2",
    "d3": "^7.6.1",
    "eslint": "^8.28.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "idb-keyval": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.15.0",
    "stylelint-config-standard": "^29.0.0",
    "tippy.js": "^6.3.7",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "stylelint": "npx stylelint \"**/0*.css\" \"browser-extension/*.css\"",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "dev-be": "npx webpack --config browser-extension/webpack.common.js --watch",
    "build-browser-extension": "npx webpack --config browser-extension/webpack.common.js",
    "compile-lib": "npx tsc -p lib --outDir dist/lib/",
    "compile-typescript-backend": "npx tsc -p backend",
    "start-server-dev": "node dist/backend/index.js --http-port 8080 --session-secret 000",
    "start-server-dev-tls": "node dist/backend/index.js --https-port 8080 --use-https --cert-path ~/.neno/server.cert --cert-key-path ~/.neno/server.key",
    "build-frontend-production": "npx webpack --config frontend/webpack.production.js",
    "build-frontend-production-stats": "npx webpack --config frontend/webpack.production.js --profile --json > stats.json",
    "build-frontend-development": "npx webpack --config frontend/webpack.development.js",
    "build": "npm run compile-typescript-backend && npm run build-frontend-production",
    "start-dev-server-frontend": "npx webpack serve --config frontend/webpack.development.js",
    "start": "npm run build && npm run start-server-dev",
    "start-tls": "npm run build && npm run start-server-dev-tls",
    "dev": "concurrently -i --default-input-target 0 --kill-others \"npm run compile-typescript-backend && npm run start-server-dev\" \"npm run build-frontend-development\"",
    "unit-test-backend": "npx tsc -p backend && node dist/backend/lib/getDocumentTitle.spec.js",
    "test-intl": "node frontend/app/intl/test.js",
    "unit-test-notes-module": "npm run compile-lib && node dist/lib/notes/index.spec.js && node dist/lib/notes/search.spec.js",
    "unit-test-unbalanced-binary-tree": "npm run compile-lib && node dist/lib/UnbalancedBinaryTree.spec.js",
    "unit-test-subwaytext": "npm run compile-lib && node dist/lib/subwaytext/index.spec.js",
    "subwaytext-example": "npm run compile-lib && node dist/lib/subwaytext/examples/example.js",
    "subwaytext-performance-test": "npm run compile-lib && node dist/lib/subwaytext/examples/performance-test.js"
  },
  "author": "Sebastian Zimmer",
  "license": "Apache License 2.0"
}
